# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'RxSwiftCourse' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RxSwiftCourse
	pod 'RxSwift'
        pod 'RxCocoa'
        pod 'Alamofire'
        pod 'SwiftyJSON'
        pod 'Firebase/Core'
        pod 'Firebase/Database'

  target 'RxSwiftCourseTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'RxSwift'
        pod 'RxCocoa'
        pod 'Alamofire'
        pod 'SwiftyJSON'
        pod 'Firebase/Core'
        pod 'Firebase/Database' 
 end

  target 'RxSwiftCourseUITests' do
    inherit! :search_paths
    # Pods for testing
	pod 'RxSwift'
        pod 'RxCocoa'
        pod 'Alamofire'
        pod 'SwiftyJSON'
        pod 'Firebase/Core'
        pod 'Firebase/Database'
  end

end
